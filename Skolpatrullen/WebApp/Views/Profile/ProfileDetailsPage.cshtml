@model WebApp.ViewModels.ProfileViewModel

@{
    ViewData["Title"] = "ProfileDetailsPage";
}

<h1>ProfileDetailsPage</h1>

<div>
    <h4>ProfileViewModel</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User.FirstName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.FirstName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User.LastNames)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.LastNames)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User.Phone)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Phone)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User.Address)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Address)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User.City)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.City)
        </dd>
    </dl>
</div>
