
@{
    ViewData["Title"] = "RegisterPage";
}

    <form>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="Email">Mailadress</label>
                <input type="email" class="form-control" placeholder="jan.jansson@gmail.com">
            </div>
            <div class="form-group col-md-6">
                <label for="Password">Lösenord</label>
                <input type="password" class="form-control" placeholder="lösenord">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="FirstName">Förnamn</label>
                <input type="text" class="form-control">
            </div>
            <div class="form-group col-md-6">
                <label for="LastName">Efternamn</label>
                <input type="text" class="form-control">
            </div>
        </div>
        <div class="form-group">
            <label>Personnummer</label>
            <input type="text" class="form-control">
        </div>
        <div class="form-group">
            <label>Gatuadress</label>
            <input type="text" class="form-control" placeholder="Sveavägen 123">
        </div>
        <div class="form-group">
            <label>Postnummer</label>
            <input type="text" class="form-control" placeholder="169 40">
        </div>
        <div class="form-group">
            <label>Stad</label>
            <input type="text" class="form-control" placeholder="Stockholm">
        </div>
        <div class="form-group">
            <label>Telefonnummer</label>
            <input type="text" class="form-control" placeholder="073 111 22 33">
        </div>
        <button type="submit" class="btn btn-primary">Registrera</button>
    </form>